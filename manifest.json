{
    "manifest_version": 2,

    "name": "Lever Greenhouse App Filler Plugin",
    "description": "This extension will fill in links to LinkedIn and GitHub profiles on job apps",
    "version": "1.0",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true,
        "open_in_tab": true
    },

    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}
